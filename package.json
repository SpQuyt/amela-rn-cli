{
  "name": "amela-rn-cli",
  "version": "0.7.0",
  "description": "AMELA react-native CLI",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prepare": "husky install",
    "start": "node index.js",
    "dev": "NODE_ENV=development node index.js --init",
    "dev-version": "NODE_ENV=development node index.js --version",
    "dev-fastlane": "NODE_ENV=development node index.js --fastlane",
    "dev-codepush": "NODE_ENV=development node index.js --codepush",
    "dev-onesignal": "NODE_ENV=development node index.js --onesignal",
    "prod": "node index.js --init",
    "prod-fastlane": "node index.js --fastlane",
    "prod-version": "node index.js --version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SpQuyt/amela-rn-cli.git"
  },
  "keywords": [],
  "author": "spquyt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SpQuyt/amela-rn-cli/issues"
  },
  "homepage": "https://github.com/SpQuyt/amela-rn-cli#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "boxen": "^5.0.1",
    "chalk": "^4.1.1",
    "clear": "^0.1.0",
    "clui": "^0.3.6",
    "colors": "^1.4.0",
    "configstore": "^6.0.0",
    "dotenv": "^16.0.1",
    "figlet": "^1.5.0",
    "inquirer": "^8.1.1",
    "lodash": "^4.17.21",
    "minimist": "^1.2.5",
    "module-alias": "^2.2.2",
    "node-keytool": "^0.0.3",
    "package-json": "^6.5.0",
    "prompt": "^1.1.0",
    "semver": "^7.3.5",
    "semver-diff": "^3.1.1",
    "sharp": "^0.28.3",
    "simple-git": "^2.40.0",
    "touch": "^3.1.0",
    "xcode": "^3.0.1",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.2.9",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3"
  },
  "bin": {
    "amela-rn-cli": "./index.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "node"
    ]
  },
  "_moduleAliases": {
    "src": "./src",
    "components": "./src/components",
    "features": "./src/features",
    "utils": "./src/utils",
    "keytool-utils": "./src/keytool-utils",
    "xcode-utils": "./src/xcode-utils",
    "icon-utils": "./src/icon-utils",
    "fastlane-utils": "./src/fastlane-utils",
    "validate": "./src/validate"
  }
}
